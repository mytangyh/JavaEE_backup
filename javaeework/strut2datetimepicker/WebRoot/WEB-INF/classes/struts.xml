<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
 	<constant name="struts.devMode" value="true" />
	
	<package name="default" namespace="/" extends="struts-default">
		
		<action name="dateTimePickerAction" 
			class="com.yiibai.common.action.DateTimePickerAction" method="display">
			<result name="none">/pages/datetimepicker.jsp</result>
		</action>
		
		<action name="resultAction" class="com.yiibai.common.action.DateTimePickerAction" >
			<result name="success">/pages/result.jsp</result>
		</action>
	</package>
</struts>    
